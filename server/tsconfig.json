{
  "compilerOptions": {                
    "target": "ES2017", 
    "module": "commonjs", 
    "outDir": "./tsc",               
    "strict": false,                           
    "noImplicitAny": true,
    "esModuleInterop": true, 
    "experimentalDecorators": true,   
    "skipLibCheck": true,                   
    "forceConsistentCasingInFileNames": true,
    "suppressImplicitAnyIndexErrors":true,
  }
}
